{
  "name": "vue-learnjavascript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "json": "json-server --watch src/assets/db.json"
  },
  "dependencies": {
    "bootstrap": "4.2.1",
    "deep-equal": "1.0.1",
    "dropzone": "5.5.1",
    "flatpickr": "4.5.2",
    "nprogress": "0.2.0",
    "vue": "^2.6.0",
    "vue-nprogress": "0.1.5",
    "vue-router": "3.0.2",
    "vue2-editor": "2.6.6",
    "vuelidate": "0.7.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.4.0",
    "@vue/cli-plugin-eslint": "3.4.0",
    "@vue/cli-service": "3.4.0",
    "@vue/eslint-config-prettier": "4.0.1",
    "babel-eslint": "10.0.1",
    "eslint": "5.13.0",
    "eslint-plugin-vue": "5.1.0",
    "lint-staged": "8.1.3",
    "prettier": "1.16.4",
    "vue-template-compiler": "2.6.6",
    "acorn": "6.1.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
